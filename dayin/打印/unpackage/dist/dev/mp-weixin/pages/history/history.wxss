
.container.data-v-b2d018fa {
		min-height: 100vh;
		background-color: #f5f5f5;
}
	
	/* 筛选栏 */
.filter-bar.data-v-b2d018fa {
		display: flex;
		background-color: #ffffff;
		padding: 20rpx;
		gap: 20rpx;
}
.filter-item.data-v-b2d018fa {
		flex: 1;
		padding: 20rpx;
		background-color: #f5f5f5;
		border-radius: 12rpx;
		display: flex;
		justify-content: space-between;
		align-items: center;
		font-size: 28rpx;
		color: #333333;
}
.arrow.data-v-b2d018fa {
		font-size: 20rpx;
		color: #999999;
}
	
	/* 历史列表 */
.history-list.data-v-b2d018fa {
		padding: 20rpx;
}
.history-item.data-v-b2d018fa {
		background-color: #ffffff;
		border-radius: 20rpx;
		padding: 30rpx;
		margin-bottom: 20rpx;
		display: flex;
		justify-content: space-between;
		align-items: center;
}
.item-left.data-v-b2d018fa {
		display: flex;
		align-items: center;
		flex: 1;
}
.item-icon.data-v-b2d018fa {
		font-size: 70rpx;
		margin-right: 30rpx;
}
.item-info.data-v-b2d018fa {
		display: flex;
		flex-direction: column;
}
.item-title.data-v-b2d018fa {
		font-size: 32rpx;
		font-weight: bold;
		color: #333333;
		margin-bottom: 10rpx;
}
.item-printer.data-v-b2d018fa {
		font-size: 26rpx;
		color: #666666;
		margin-bottom: 10rpx;
}
.item-time.data-v-b2d018fa {
		font-size: 24rpx;
		color: #999999;
}
.item-right.data-v-b2d018fa {
		display: flex;
		align-items: center;
}
.item-status.data-v-b2d018fa {
		padding: 8rpx 20rpx;
		border-radius: 12rpx;
		font-size: 24rpx;
		margin-right: 20rpx;
}
.item-status.success.data-v-b2d018fa {
		background-color: #f6ffed;
		color: #52c41a;
}
.item-status.failed.data-v-b2d018fa {
		background-color: #fff1f0;
		color: #ff4d4f;
}
.item-status.pending.data-v-b2d018fa {
		background-color: #fff7e6;
		color: #faad14;
}
	
	/* 加载更多 */
.load-more.data-v-b2d018fa {
		padding: 20rpx;
		text-align: center;
}
.load-btn.data-v-b2d018fa {
		width: 400rpx;
		height: 70rpx;
		line-height: 70rpx;
		background-color: #ffffff;
		border: 1px solid #d9d9d9;
		border-radius: 12rpx;
		font-size: 28rpx;
		color: #666666;
}
	
	/* 对话框 */
.dialog-mask.data-v-b2d018fa {
		position: fixed;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;
		background-color: rgba(0, 0, 0, 0.5);
		display: flex;
		align-items: center;
		justify-content: center;
		z-index: 9999;
}
.dialog.data-v-b2d018fa {
		width: 640rpx;
		background-color: #ffffff;
		border-radius: 20rpx;
		overflow: hidden;
}
.dialog-title.data-v-b2d018fa {
		font-size: 36rpx;
		font-weight: bold;
		color: #333333;
		padding: 40rpx 30rpx 20rpx;
		text-align: center;
}
.detail-content.data-v-b2d018fa {
		padding: 20rpx 30rpx 40rpx;
}
.detail-item.data-v-b2d018fa {
		display: flex;
		justify-content: space-between;
		padding: 20rpx 0;
		border-bottom: 1px solid #f0f0f0;
}
.detail-item.data-v-b2d018fa:last-child {
		border-bottom: none;
}
.detail-label.data-v-b2d018fa {
		font-size: 28rpx;
		color: #999999;
}
.detail-value.data-v-b2d018fa {
		font-size: 28rpx;
		color: #333333;
		text-align: right;
		max-width: 400rpx;
		word-break: break-all;
}
.status-success.data-v-b2d018fa {
		color: #52c41a !important;
}
.status-failed.data-v-b2d018fa {
		color: #ff4d4f !important;
}
.status-pending.data-v-b2d018fa {
		color: #faad14 !important;
}
.dialog-footer.data-v-b2d018fa {
		display: flex;
		border-top: 1px solid #f0f0f0;
}
.close-btn.data-v-b2d018fa,
	.reprint-btn.data-v-b2d018fa {
		flex: 1;
		height: 100rpx;
		line-height: 100rpx;
		border: none;
		font-size: 32rpx;
		background-color: #ffffff;
}
.close-btn.data-v-b2d018fa {
		color: #666666;
}
.reprint-btn.data-v-b2d018fa {
		color: #1890ff;
		border-left: 1px solid #f0f0f0;
}
.empty-tip.data-v-b2d018fa {
		text-align: center;
		padding: 120rpx 0;
		color: #999999;
		font-size: 28rpx;
}
