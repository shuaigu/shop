
.container.data-v-4bff6566 {
		min-height: 100vh;
		background-color: #f5f5f5;
		padding: 20rpx;
}
	
	/* 打印机列表 */
.printer-list.data-v-4bff6566 {
		margin-bottom: 20rpx;
}
.printer-item.data-v-4bff6566 {
		background-color: #ffffff;
		border-radius: 20rpx;
		padding: 30rpx;
		margin-bottom: 20rpx;
		display: flex;
		justify-content: space-between;
		align-items: center;
		border: 2px solid transparent;
		transition: all 0.3s;
}
.printer-item.selected.data-v-4bff6566 {
		border-color: #1890ff;
		background-color: #e6f7ff;
}
.printer-main.data-v-4bff6566 {
		display: flex;
		align-items: center;
		flex: 1;
}
.printer-icon.data-v-4bff6566 {
		font-size: 80rpx;
		margin-right: 30rpx;
}
.printer-info.data-v-4bff6566 {
		display: flex;
		flex-direction: column;
}
.printer-name.data-v-4bff6566 {
		font-size: 32rpx;
		font-weight: bold;
		color: #333333;
		margin-bottom: 10rpx;
}
.printer-model.data-v-4bff6566 {
		font-size: 26rpx;
		color: #999999;
}
.printer-status.data-v-4bff6566 {
		display: flex;
		align-items: center;
		padding: 10rpx 20rpx;
		border-radius: 20rpx;
		font-size: 24rpx;
}
.printer-status.online.data-v-4bff6566 {
		background-color: #f6ffed;
		color: #52c41a;
}
.printer-status.offline.data-v-4bff6566 {
		background-color: #fff1f0;
		color: #ff4d4f;
}
.status-dot.data-v-4bff6566 {
		width: 12rpx;
		height: 12rpx;
		border-radius: 50%;
		margin-right: 10rpx;
}
.printer-status.online .status-dot.data-v-4bff6566 {
		background-color: #52c41a;
}
.printer-status.offline .status-dot.data-v-4bff6566 {
		background-color: #ff4d4f;
}
	
	/* 添加按钮 */
.add-section.data-v-4bff6566 {
		padding: 20rpx 0;
}
.add-btn.data-v-4bff6566 {
		width: 100%;
		height: 88rpx;
		line-height: 88rpx;
		background-color: #1890ff;
		color: #ffffff;
		border: none;
		border-radius: 12rpx;
		font-size: 32rpx;
}
	
	/* 对话框 */
.dialog-mask.data-v-4bff6566 {
		position: fixed;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;
		background-color: rgba(0, 0, 0, 0.5);
		display: flex;
		align-items: center;
		justify-content: center;
		z-index: 9999;
}
.dialog.data-v-4bff6566 {
		width: 640rpx;
		background-color: #ffffff;
		border-radius: 20rpx;
		overflow: hidden;
}
.dialog-title.data-v-4bff6566 {
		font-size: 36rpx;
		font-weight: bold;
		color: #333333;
		padding: 40rpx 30rpx 20rpx;
		text-align: center;
}
.quick-fill-section.data-v-4bff6566 {
		padding: 0 30rpx 20rpx;
}
.quick-fill-btn.data-v-4bff6566 {
		width: 100%;
		height: 70rpx;
		line-height: 70rpx;
		background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
		color: #ffffff;
		border: none;
		border-radius: 12rpx;
		font-size: 28rpx;
		padding: 0;
}
.dialog-content.data-v-4bff6566 {
		padding: 20rpx 30rpx;
		max-height: 800rpx;
		overflow-y: auto;
}
.form-item.data-v-4bff6566 {
		margin-bottom: 30rpx;
}
.label.data-v-4bff6566 {
		display: block;
		font-size: 28rpx;
		color: #333333;
		margin-bottom: 15rpx;
}
.input.data-v-4bff6566 {
		width: 100%;
		height: 80rpx;
		padding: 0 20rpx;
		border: 1px solid #d9d9d9;
		border-radius: 8rpx;
		font-size: 28rpx;
}
.scan-row.data-v-4bff6566 {
		display: flex;
		align-items: center;
		gap: 20rpx;
}
.flex-input.data-v-4bff6566 {
		flex: 1;
}
.scan-btn.data-v-4bff6566 {
		width: 140rpx;
		height: 80rpx;
		line-height: 80rpx;
		background-color: #1890ff;
		color: #ffffff;
		border: none;
		border-radius: 8rpx;
		font-size: 28rpx;
		padding: 0;
}
.dialog-footer.data-v-4bff6566 {
		display: flex;
		border-top: 1px solid #f0f0f0;
}
.cancel-btn.data-v-4bff6566,
	.confirm-btn.data-v-4bff6566 {
		flex: 1;
		height: 100rpx;
		line-height: 100rpx;
		border: none;
		font-size: 32rpx;
}
.cancel-btn.data-v-4bff6566 {
		background-color: #ffffff;
		color: #666666;
}
.confirm-btn.data-v-4bff6566 {
		background-color: #ffffff;
		color: #1890ff;
		border-left: 1px solid #f0f0f0;
}
.empty-tip.data-v-4bff6566 {
		text-align: center;
		padding: 120rpx 0;
		color: #999999;
}
.empty-tip text.data-v-4bff6566 {
		display: block;
		font-size: 28rpx;
}
.sub-tip.data-v-4bff6566 {
		font-size: 24rpx;
		margin-top: 20rpx;
}
