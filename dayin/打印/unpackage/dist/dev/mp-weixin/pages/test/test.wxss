
.container.data-v-727d09f0 {
		min-height: 100vh;
		background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
		padding: 20rpx;
}
.header.data-v-727d09f0 {
		text-align: center;
		padding: 40rpx 0;
}
.title.data-v-727d09f0 {
		font-size: 48rpx;
		font-weight: bold;
		color: #ffffff;
}
	
	/* 设备信息卡片 */
.device-card.data-v-727d09f0 {
		background-color: #ffffff;
		border-radius: 20rpx;
		padding: 30rpx;
		margin-bottom: 20rpx;
}
.card-title.data-v-727d09f0 {
		font-size: 32rpx;
		font-weight: bold;
		color: #333333;
		margin-bottom: 20rpx;
}
.info-row.data-v-727d09f0 {
		display: flex;
		padding: 15rpx 0;
		border-bottom: 1px solid #f0f0f0;
}
.info-row.data-v-727d09f0:last-child {
		border-bottom: none;
}
.label.data-v-727d09f0 {
		font-size: 28rpx;
		color: #666666;
		width: 180rpx;
}
.value.data-v-727d09f0 {
		font-size: 28rpx;
		color: #333333;
		flex: 1;
		word-break: break-all;
}
.value.disconnected.data-v-727d09f0 {
		color: #999999;
}
.value.connecting.data-v-727d09f0 {
		color: #faad14;
}
.value.connected.data-v-727d09f0 {
		color: #52c41a;
}
.value.error.data-v-727d09f0 {
		color: #ff4d4f;
}
	
	/* 步骤卡片 */
.steps-card.data-v-727d09f0 {
		background-color: #ffffff;
		border-radius: 20rpx;
		padding: 30rpx;
		margin-bottom: 20rpx;
}
	
	/* 查询卡片 */
.query-card.data-v-727d09f0 {
		background-color: #ffffff;
		border-radius: 20rpx;
		padding: 30rpx;
		margin-bottom: 20rpx;
}
.task-id.data-v-727d09f0 {
		font-family: 'Courier New', monospace;
		font-size: 24rpx;
		word-break: break-all;
}
.action-row.data-v-727d09f0 {
		display: flex;
		gap: 15rpx;
		margin-top: 20rpx;
}
.query-btn.data-v-727d09f0, .cancel-btn.data-v-727d09f0, .preview-btn.data-v-727d09f0 {
		flex: 1;
		height: 70rpx;
		line-height: 70rpx;
		background-color: #1890ff;
		color: #ffffff;
		border: none;
		border-radius: 12rpx;
		font-size: 26rpx;
		padding: 0;
}
.cancel-btn.data-v-727d09f0 {
		background-color: #ff4d4f;
}
.preview-btn.data-v-727d09f0 {
		background-color: #52c41a;
}
.task-info.data-v-727d09f0 {
		margin-top: 20rpx;
		padding: 20rpx;
		background-color: #f5f5f5;
		border-radius: 12rpx;
}
.status-success.data-v-727d09f0 {
		color: #52c41a !important;
		font-weight: bold;
}
.status-error.data-v-727d09f0 {
		color: #ff4d4f !important;
		font-weight: bold;
}
.status-processing.data-v-727d09f0 {
		color: #faad14 !important;
		font-weight: bold;
}
.step-item.data-v-727d09f0 {
		display: flex;
		align-items: center;
		padding: 20rpx 0;
		opacity: 0.5;
}
.step-item.active.data-v-727d09f0 {
		opacity: 1;
}
.step-item.success .step-num.data-v-727d09f0 {
		background-color: #52c41a;
}
.step-num.data-v-727d09f0 {
		width: 60rpx;
		height: 60rpx;
		line-height: 60rpx;
		text-align: center;
		background-color: #d9d9d9;
		color: #ffffff;
		border-radius: 50%;
		font-size: 28rpx;
		font-weight: bold;
		margin-right: 20rpx;
}
.step-content.data-v-727d09f0 {
		flex: 1;
		display: flex;
		flex-direction: column;
}
.step-title.data-v-727d09f0 {
		font-size: 30rpx;
		color: #333333;
		margin-bottom: 5rpx;
}
.step-desc.data-v-727d09f0 {
		font-size: 24rpx;
		color: #999999;
}
.step-icon.data-v-727d09f0 {
		font-size: 40rpx;
		color: #52c41a;
		width: 60rpx;
		text-align: center;
}
	
	/* 日志卡片 */
.log-card.data-v-727d09f0 {
		background-color: #ffffff;
		border-radius: 20rpx;
		padding: 30rpx;
		margin-bottom: 20rpx;
}
.log-scroll.data-v-727d09f0 {
		height: 500rpx;
		background-color: #1e1e1e;
		border-radius: 12rpx;
		padding: 20rpx;
}
.log-item.data-v-727d09f0 {
		display: flex;
		margin-bottom: 10rpx;
		font-size: 24rpx;
		font-family: 'Courier New', monospace;
}
.log-time.data-v-727d09f0 {
		color: #888888;
		margin-right: 15rpx;
		min-width: 120rpx;
}
.log-text.data-v-727d09f0 {
		flex: 1;
		color: #ffffff;
}
.log-item.info .log-text.data-v-727d09f0 {
		color: #61dafb;
}
.log-item.success .log-text.data-v-727d09f0 {
		color: #52c41a;
}
.log-item.error .log-text.data-v-727d09f0 {
		color: #ff4d4f;
}
.log-item.warning .log-text.data-v-727d09f0 {
		color: #faad14;
}
.empty-log.data-v-727d09f0 {
		text-align: center;
		color: #666666;
		padding: 100rpx 0;
}
	
	/* 操作按钮 */
.action-buttons.data-v-727d09f0 {
		padding: 20rpx 0 40rpx;
		display: flex;
		flex-wrap: wrap;
		gap: 20rpx;
}
.test-btn.data-v-727d09f0 {
		flex: 1;
		min-width: 300rpx;
		height: 100rpx;
		line-height: 100rpx;
		background: linear-gradient(135deg, #ff6b6b 0%, #ee5a6f 100%);
		color: #ffffff;
		border: none;
		border-radius: 50rpx;
		font-size: 32rpx;
		font-weight: bold;
		box-shadow: 0 8rpx 20rpx rgba(255, 107, 107, 0.4);
}
.test-btn[disabled].data-v-727d09f0 {
		opacity: 0.6;
}
.reset-btn.data-v-727d09f0, .web-btn.data-v-727d09f0, .printer-list-btn.data-v-727d09f0 {
		min-width: 180rpx;
		height: 100rpx;
		line-height: 100rpx;
		background-color: #ffffff;
		color: #666666;
		border: 2px solid #d9d9d9;
		border-radius: 50rpx;
		font-size: 28rpx;
}
.printer-list-btn.data-v-727d09f0 {
		color: #52c41a;
		border-color: #52c41a;
}
.web-btn.data-v-727d09f0 {
		color: #1890ff;
		border-color: #1890ff;
}
