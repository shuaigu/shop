// 点赞列表
{
	"bsonType": "object",
	"required": [ ],
	"permission": {
		"read": true,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"user_id": {
			"bsonType": "string",
			"description": "用户id",
			"foreignKey": "user._id"
		},
		"article_id": {
			"bsonType": "string",
			"description": "文章id",
			"foreignKey": "articleList._id"
		},
		"like_status": {
			"bsonType": "int",
			"description": "点赞状态，1为已点赞，0为未点赞"
		},
		"create_time": {
			"bsonType": "timestamp",
			"description": "点赞时间",
			"defaultValue": {
				"$env": "now"
			}
		}
	}
}