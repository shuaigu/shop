/*防止分包页面公共样式无法读取*/
page.data-v-b5b6feed,
view.data-v-b5b6feed,
text.data-v-b5b6feed,
image.data-v-b5b6feed,
button.data-v-b5b6feed {
  font-family: "PingFang SC", "Helvetica Neue", Arial, "Hiragino Sans GB", "Microsoft YaHei", "WenQuanYi Micro Hei", sans-serif;
  box-sizing: border-box;
  margin: 0;
  padding: 0;
}
image.data-v-b5b6feed {
  width: 100%;
  height: 100%;
}
.adminManage.data-v-b5b6feed {
  padding: 24rpx;
  height: 100vh;
  overflow: hidden;
  background-color: #f7f7f7;
}
.adminManage .content.data-v-b5b6feed {
  border-radius: 24rpx;
  background-color: #fff;
}
.adminManage .content .item.data-v-b5b6feed {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 24rpx;
  width: 100%;
  box-sizing: border-box;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}
.adminManage .content .item.data-v-b5b6feed:nth-last-child(1) {
  border: none;
}
.adminManage .content .item .left.data-v-b5b6feed {
  display: flex;
  align-items: center;
}
.adminManage .content .item .left .box.data-v-b5b6feed {
  margin-right: 16rpx;
  height: 24rpx;
  width: 8rpx;
  background-color: #399bfe;
  border-radius: 4rpx;
}
.adminManage .content .item .left .value.data-v-b5b6feed {
  font-size: 28rpx;
  color: #444444;
}
.adminManage .content .item .switch-container.data-v-b5b6feed {
  padding: 0 10rpx;
}
.adminManage .content .item .id-preview.data-v-b5b6feed {
  font-size: 24rpx;
  color: #666;
  background-color: #f5f5f5;
  padding: 4rpx 20rpx;
  border-radius: 30rpx;
  max-width: 200rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.adminManage .content .item .id-preview.data-v-b5b6feed:active {
  background-color: #e9e9e9;
}
.adminManage .modal-container.data-v-b5b6feed {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9999;
  display: flex;
  align-items: center;
  justify-content: center;
}
.adminManage .modal-container .modal-mask.data-v-b5b6feed {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: -1;
}
.adminManage .modal-container .modal-content.data-v-b5b6feed {
  width: 600rpx;
  background-color: #fff;
  border-radius: 20rpx;
  overflow: hidden;
  box-shadow: 0 4rpx 20rpx rgba(0, 0, 0, 0.15);
}
.adminManage .modal-container .modal-content .modal-header.data-v-b5b6feed {
  padding: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #eee;
}
.adminManage .modal-container .modal-content .modal-header .modal-title.data-v-b5b6feed {
  font-size: 32rpx;
  font-weight: 500;
  color: #333;
}
.adminManage .modal-container .modal-content .modal-header .modal-close.data-v-b5b6feed {
  width: 44rpx;
  height: 44rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
}
.adminManage .modal-container .modal-content .modal-header .modal-close.data-v-b5b6feed:active {
  background-color: #f5f5f5;
}
.adminManage .modal-container .modal-content .modal-body.data-v-b5b6feed {
  padding: 30rpx;
}
.adminManage .modal-container .modal-content .modal-body .input-group.data-v-b5b6feed {
  margin-bottom: 20rpx;
}
.adminManage .modal-container .modal-content .modal-body .input-group .input-textarea.data-v-b5b6feed {
  width: 100%;
  min-height: 160rpx;
  background-color: #f8f8f8;
  border-radius: 12rpx;
  padding: 20rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  line-height: 1.5;
  border: 1px solid #eee;
}
.adminManage .modal-container .modal-content .modal-body .id-tip.data-v-b5b6feed {
  font-size: 24rpx;
  color: #ff9900;
  line-height: 1.5;
}
.adminManage .modal-container .modal-content .modal-footer.data-v-b5b6feed {
  padding: 20rpx 30rpx 40rpx;
  display: flex;
  justify-content: flex-end;
}
.adminManage .modal-container .modal-content .modal-footer button.data-v-b5b6feed {
  padding: 16rpx 40rpx;
  margin-left: 20rpx;
  font-size: 28rpx;
  border-radius: 40rpx;
  background-color: transparent;
}
.adminManage .modal-container .modal-content .modal-footer button.data-v-b5b6feed::after {
  border: none;
}
.adminManage .modal-container .modal-content .modal-footer .btn-cancel.data-v-b5b6feed {
  color: #666;
  background-color: #f5f5f5;
}
.adminManage .modal-container .modal-content .modal-footer .btn-cancel.data-v-b5b6feed:active {
  opacity: 0.8;
}
.adminManage .modal-container .modal-content .modal-footer .btn-save.data-v-b5b6feed {
  color: #fff;
  background-color: #399bfe;
  box-shadow: 0 4rpx 8rpx rgba(57, 155, 254, 0.3);
}
.adminManage .modal-container .modal-content .modal-footer .btn-save.data-v-b5b6feed:active {
  opacity: 0.8;
}